var myChart = new Chart(ctx, {
    type: 'line',
    data: {
        labels: {{date|safe}},
        datasets: [{
            label: 'New cases',
            data: {{cases|safe}},
            //backgroundColor: "red",
        }]
    },
    options: {
    }
});
</script>
